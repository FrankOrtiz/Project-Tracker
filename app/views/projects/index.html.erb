<div class="row">
  <div class="col-sm-12">
    <h1>Active Lab Projects</h1>
     <% @projects.each do |project| %>
      <div class="thumbnail"> 
        <div class="caption">         
        <span class="thumb-title"><strong><%= project.name %></strong></span>  </div>
        <div class="well project-sm" href="<%= project %>" >
        
      
      <p class="thumb-description"><%= project.description %></p>  
    </div>
    <p class="thumb-who"><%= project.who %></p>
  </div>
    <% end %>
  </div>
</div>

  <div class="btn-group">
  <%= link_to 'Create Project', new_project_path, class:'btn btn-primary btn-sm' %>
</div>
